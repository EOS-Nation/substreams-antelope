{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch consumers/node",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/consumers/node/src/index.ts",
            "preLaunchTask": "npm: build - consumers/node",
            "args": ["run", "${workspaceFolder}/substreams/tokens/tokens-v0.0.1.spkg", "--modules", "store_tokens", "--start-block", "186", "--stop-block", "200"],
            "sourceMaps": true,
            "smartStep": true,
            "internalConsoleOptions": "openOnSessionStart",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}